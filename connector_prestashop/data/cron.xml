<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data> <!-- noupdate="1" -->

        <!-- CRON - import customers -->
        <record id="cron_prestashop_import_all" model="ir.cron">
            <field name="name">cron.prestashop.import.all</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doal">1</field>
            <field name="model_id" ref="model_prestashop_backend" />
            <field name="code">model.cron_import_all()</field>
            <field name="state">code</field>
            <field name="type">ir.actions.server</field>
        </record>

